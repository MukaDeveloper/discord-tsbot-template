{
  "name": "discord-tsbot-template",
  "version": "1.0.0",
  "description": "Template para bot do Discord em TypeScript",
  "main": "dist/main.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "start": "npm run build && node dist/main.js",
    "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "build": "tsc && tsc-alias -p tsconfig.json",
    "watch": "tsc -w"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "muka",
    "mxk4"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
